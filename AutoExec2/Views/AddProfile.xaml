<Window x:Class="AutoExec2.Views.AddProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoExec2.Views"
        mc:Ignorable="d"
        Title="AddProfile" Height="160" Width="200" MinWidth="200" MinHeight="160" ResizeMode="NoResize" ShowInTaskbar="False" Closed="Window_Closed">
    <Grid>
        <Label Content="Add profile" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" FontWeight="Bold" FontSize="22"/>
        <TextBox x:Name="newNameText" Height="23" Margin="59,56,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Label Content="Name:" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="addButton" Content="Add" Margin="10,85,10,10" Click="addButton_Click"/>

    </Grid>
</Window>
